{
  "name": "rabbitmq",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker run -d --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:management"
      }
    },
    "stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker stop rabbitmq && docker rm rabbitmq"
      }
    }
  }
}
